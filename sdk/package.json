{
  "name": "@eddmann/pwa-kit-sdk",
  "version": "0.1.0",
  "description": "JavaScript SDK for PWAKit native bridge",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "pwa",
    "ios",
    "native",
    "bridge",
    "webview",
    "haptics",
    "notifications",
    "biometrics",
    "healthkit",
    "in-app-purchases"
  ],
  "author": "Edd Mann",
  "license": "MIT",
  "devDependencies": {
    "@vitest/coverage-v8": "^2.0.0",
    "jsdom": "^25.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eddmann/pwa-kit.git",
    "directory": "sdk"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  }
}
